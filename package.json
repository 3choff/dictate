{
  "name": "dictate",
  "version": "0.6.7",
  "main": "src/main/main.js",
  "scripts": {
    "start": "electron .",
    "rebuild": "electron-rebuild -f -w robotjs",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "electron-builder",
    "sync-doc-version": "node scripts/sync-doc-version.js",
    "postversion": "npm run sync-doc-version && git add README.md"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "build": {
    "appId": "com.3choff.dictate",
    "productName": "Dictate",
    "asar": true,
    "files": [
      "src/**/*",
      "!src/**/*.map",
      "node_modules/axios/**/*",
      "node_modules/electron-store/**/*",
      "node_modules/form-data/**/*",
      "node_modules/robotjs/**/*",
      "package.json"
    ],
    "extraResources": [
      "assets/**/*"
    ],
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "portable",
      "icon": "build/icon.ico"
    }
  },
  "devDependencies": {
    "electron": "^38.1.2",
    "electron-builder": "^26.0.12",
    "electron-rebuild": "^3.2.9"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "electron-store": "^10.1.0",
    "form-data": "^4.0.4",
    "robotjs": "^0.6.0"
  }
}
